package gvprojects.chess.model;
/**
 * 
 * @author rohrj
 *
 */
public class Knight extends ChessPiece{

	public Knight(Player p) {
		super(p, "Knight");
	}

	@Override
	public boolean isValidMove(Move m, IChessPiece[][] p) {
		if (super.isValidMove(m, p) == false)
			return false;
		
		return true;
	}
}
